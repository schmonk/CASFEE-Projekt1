{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "warn",
        "no-unused-vars":"warn",
        "import/extensions": ["error", "always", {"ignorePackages": true} ]
    }
}
